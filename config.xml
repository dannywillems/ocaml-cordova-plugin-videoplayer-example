<?xml version='1.0' encoding='utf-8'?>
<widget id="be.dannywillems.ocamlcordova.videoplayer" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>OCaml Video player</name>
    <description>
        A sample Apache Cordova application which shows the binding to the videoplayer cordova plugin in OCaml
    </description>
    <author email="contact@danny-willems.be" href="https://danny-willems.be">
      Danny Willems
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
